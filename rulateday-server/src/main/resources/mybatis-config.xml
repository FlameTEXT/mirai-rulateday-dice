<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <!-- 加载数据库配置 -->

    <environments default="development">
        <environment id="development">
            <transactionManager type="JDBC"/>
            <dataSource type="POOLED">
                <property name="driver" value="${driver}"/>
                <property name="url" value="${url}"/>
            </dataSource>
        </environment>
    </environments>

    <mappers>
        <mapper class="com.github.eiriksgata.rulateday.mapper.UserTempDataMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.CrazyDescribeMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.CrazyOverDescribeMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.RuleBookMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.SpeakersGroupListMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.Dnd5ePhbTestBaseMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.Dnd5ePhbDataMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.UserConversationMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.UserInitiativeDataMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.CardsGroupDataMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.CardsTypeListMapper"/>
        <mapper class="com.github.eiriksgata.rulateday.mapper.DiceConfigMapper"/>
    </mappers>

</configuration>
